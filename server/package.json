    "http-status-codes": "^2.1.4",
    "inversify": "^5.0.5",
    "joi": "^17.4.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
