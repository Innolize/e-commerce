  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc-watch --onSuccess \"node . \"",
    "dev": "tsc -w & node .",
    "dbconfig": "tsc-watch --onSuccess \"node ./dist/config/database/dbConfig.js\"",
